@model Payxat.Repository.DataObject.ContactU
@{
    ViewBag.Title = "GetContactUsDetails";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<!-- Dashboard Counts Section-->
<section class="dashboard-counts no-padding-bottom">
    <div class="container-fluid">
        <div class="row bg-white has-shadow">

            <!-- Item -->
            <div class="col-xl-3 col-sm-6">

                <div class="item d-flex align-items-center" id="Rel-1">
                    <div class="icon bg-violet"><i class="icon-user"></i></div>
                    <div class="title">
                        <span>Project Details</span>
                        <div class="progress">
                            <div role="progressbar" style="width: 25%; height: 4px;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100" class="progress-bar bg-violet"></div>
                        </div>
                    </div>
                    <div class="number"><strong></strong></div>
                </div>

            </div>




        </div>
    </div>
</section>




<section class="forms" id="ProductInformation">
    <div class="container-fluid">

        <div class="row">

            <!-- Horizontal Form-->
            <div class="col-lg-12">

                <div class="card">
                    <div class="card-close">
                        <div class="dropdown">
                            <button type="button" id="closeCard1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" class="dropdown-toggle"><i class="fa fa-ellipsis-v"></i></button>
                            <div aria-labelledby="closeCard1" class="dropdown-menu dropdown-menu-right has-shadow"><a href="#" class="dropdown-item remove"> <i class="fa fa-times"></i>Close</a><a href="#" class="dropdown-item edit"> <i class="fa fa-gear"></i>Edit</a></div>
                        </div>
                    </div>
                    <div class="card-header  align-items-center ">
                        <div class="row">
                            <div class="col-lg-12">

                                <h3 class="h4">Contact Details</h3>
                            </div>

                        </div>
                    </div>



                    <div class="row">
                        <div class="col-lg-12">

                            <div class="card-body">

                                <form>
                                    <div class="row">




                                        <div class="col-lg-6">
                                            <div class="form-group product-form">
                                                <label class="form-control-label" style="font-weight:bold">User Name: </label>
                                                <label class="form-control-label">@Model.c_Name </label>
                                            </div>
                                        </div>



                                        <div class="col-lg-6">
                                            <div class="form-group product-form">
                                                <label class="form-control-label" style="font-weight:bold">Email:</label>
                                                <label class="form-control-label"> @Model.c_Email</label>
                                            </div>
                                        </div>

                                        <div class="col-lg-12">
                                            <div class="form-group">
                                                <label class="form-control-label" style="font-weight:bold">Phone: </label>
                                                <label class="form-control-label"> @Model.c_Phone</label>
                                            </div>
                                        </div>


                                        <div class="col-lg-12">
                                            <div class="form-group">
                                                <label class="form-control-label" style="font-weight:bold">Message:  </label>
                                                <label class="form-control-label"> @Model.c_Message</label>
                                            </div>
                                        </div>

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label class="form-control-label" style="font-weight:bold">Created Date:  </label>
                                                <label class="form-control-label">@Model.c_Date</label>
                                            </div>
                                        </div>


                                    </div>

                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Second Col -->
